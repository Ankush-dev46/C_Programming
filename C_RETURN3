#include <stdio.h>

int Find_Max(int num1, int num2) {
    if (num1 > num2) {
        return num1;
    } else {
        return num2;
    }
}

int main() {
    int num1, num2;
    
    printf("\nENTER A NUMBER (NUM1): ");
    scanf("%d", &num1);
    
    printf("ENTER A NUMBER (NUM2): ");
    scanf("%d", &num2);
    
    int max = Find_Max(num1, num2);
    
    if (num1 == max && num1 != num2) {
        printf("NUM1 (%d) IS GREATER THAN NUM2 (%d)\n", num1, num2);
    } else if (num1 == num2) {
        printf("NUM1 (%d) IS EQUAL TO NUM2 (%d)\n", num1, num2);
    } else {
        printf("NUM1 (%d) IS NOT GREATER THAN NUM2 (%d)\n", num1, num2);
    }

    return 0;
}
