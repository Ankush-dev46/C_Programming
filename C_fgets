#include <stdio.h>
#include <string.h>

int main()
{
    char s[100], name[100];

    printf("\nENTER THE NAME:");
    fgets(s, 100, stdin);
    s[strlen(s) - 1] = '\0';
    printf("THe name is%s\n", s);

    printf("\nENTER THE 2 NAME:");
    fgets(name, sizeof(name), stdin);
    name[strlen(name) - 1] = '\0';
    printf("THe name is %s\n", name);

    return 0;
}
