#include <stdio.h>

int main() {
    int x = 2, y = 3, z = 6;
    
    x++;      // x = 3
    z++;      // z = 7
    x += 4;   // x = 7
    y += 3;   // y = 6
    y -= 3;   // y = 3
    
    int a = x - y;
    int b = x * y;
    int c = x + z;
    int d = z / y; // integer division
    
    printf("a: %d\n", a);
    printf("b: %d\n", b);
    printf("c: %d\n", c);
    printf("d: %d\n", d);
    printf("x: %d\n", x);
    printf("y: %d\n", y);
    printf("z: %d\n", z);

    return 0;
}
