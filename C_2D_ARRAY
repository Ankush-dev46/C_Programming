#include <stdio.h>
int main() {
//2D ARRAY'S =AN ARRAY WHERE EACH ELEMENT IS AN ARRAY 
//ARRAY [][] = {{},{},{}};
    int arr[3][3] = {
        {1,2,3},
        {4,5,6},
        {7,8,9}
    };
    int i, j;

    printf("Element at arr[1][1]: %d\n", arr[1][1]);

    for(i = 0; i < 3; i++) {
        for(j = 0; j < 3; j++) {
            printf("%d ", arr[i][j]);
        }
        printf("\n");
    }

    char arr2[3][4] = {"abc", "def", "ghi"};
    printf("\nCharacter at arr2[1][1]: %c\n", arr2[1][1]);

    for(i = 0; i < 3; i++) {
        for(j = 0; j < 3; j++) {
            printf("%c ", arr2[i][j]);
        }
        printf("\n");
    }

    return 0;
}

